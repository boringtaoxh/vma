div(ng-controller='brandCtrl')
  preload
    #brandStory(ng-show='ready' ng-cloak)
      .col-xs-3
        include brand-sidebar
      .col-xs-9
        .videogular-container
          .video(video-player)
        .article-container
          h4 {{brandTitle}}
          #story(ng-bind-html='brandStory')