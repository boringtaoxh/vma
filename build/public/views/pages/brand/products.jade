#brandProducts(ng-controller='brandCtrl' ng-cloak)
  .preload(ng-hide='ready')
    img(src='/assets/img/ui/spinner.gif')
    h4 Please give us some time to collect lovely things...
  .wrap(ng-show='ready')
    .col-xs-3
      include brand-sidebar
    #products.col-xs-9(ng-controller='productsCtrl')
      wrap-big-products(products='brandProducts')